{
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios --project-path '../ios'",
    "android": "react-native run-android --root '..'",
    "emulator": "emulator -avd react-native -gpu on",
    "bundle:ios": "react-native bundle --entry-file ./index.ios.js --bundle-output ./dist/bundle.ios/index.ios.jsbundle --platform ios --assets-dest ./dist/bundle.ios --dev false",
    "bundle:android": "react-native bundle --entry-file ./index.android.js --bundle-output ../android/app/src/main/assets/bundle.android/index.android.jsbundle --platform android --assets-dest ../android/app/src/main/assets/bundle.android --dev false",
    "remotedev": "remotedev --hostname=localhost --port=5678"
  },
  "dependencies": {
    "mo2js": "^0.2.4",
    "npm": "^4.0.3",
    "react": "^15.4.1",
    "react-native": "^0.38.0",
    "react-native-storage": "^0.1.4",
    "react-native-vector-icons": "^3.0.0",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "remote-redux-devtools": "^0.5.7",
    "remotedev-server": "^0.3.0-beta-3"
  }
}